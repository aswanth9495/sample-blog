<h1> Edit Post </h1>
<%= form_for :post, url: post_path(@post), method: :patch do |f| %>
<%= f %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :body , {:class => "form-control"}%><br>
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit %><br>
  </p>
<%end %>